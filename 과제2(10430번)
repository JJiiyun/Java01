import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int a = in.nextInt();
        int b = in.nextInt();
        int c = in.nextInt();
        
        System.out.printf("%d%n", (a+b)%c);
        System.out.printf("%d%n", ((a%c) + (b%c))%c);
        System.out.printf("%d%n", (a*b)%c);
        System.out.printf("%d%n", ((a%c) * (b%c))%c);
        
        
    }
}
